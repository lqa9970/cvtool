{
  "name": "cvhub-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "env-cmd -f .env.development vite",
    "dev:clean": "npm run dev -- --force",
    "build": "tsc && vite build",
    "build:dev": "env-cmd -f .env.development npm run build",
    "build:prod": "env-cmd -f .env.production npm run build",
    "preview": "vite preview",
    "serve": "vite preview --port 3000",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:silent": "npm run test -- --silent",
    "lint": "eslint src",
    "lint:quiet": "npm run lint -- --quiet"
  },
  "engines": {
    "node": ">=18.12.0",
    "npm": ">=8.19.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@nordcloud/eslint-config-pat": "^5.0.0",
    "@nordcloud/gnui": "^8.1.1",
    "@okta/okta-auth-js": "^7.2.0",
    "@okta/okta-react": "^6.7.0",
    "@react-pdf/renderer": "^3.1.12",
    "@types/firebase": "^3.2.1",
    "axios": "^1.3.4",
    "formik": "^2.2.9",
    "idb": "^7.1.1",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "sass": "^1.60.0",
    "semantic-ui-calendar-react-yz": "^0.16.6",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.4",
    "styled-components": "^5.3.5",
    "stylelint-config-standard-scss": "^9.0.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.8",
    "env-cmd": "^10.1.0",
    "eslint": "^8.36.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^21.1.1",
    "patch-package": "^6.4.7",
    "prettier": "^2.8.4",
    "stylelint": "^15.7.0",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vitest": "^0.30.1"
  }
}
